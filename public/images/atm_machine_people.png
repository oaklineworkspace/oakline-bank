
<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="skyGrad" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E0F6FF;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="atmGrad" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#2C3E50;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#34495E;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="url(#skyGrad)"/>
  
  <!-- Ground -->
  <rect x="0" y="500" width="800" height="100" fill="#8FBC8F"/>
  
  <!-- ATM Machine -->
  <rect x="300" y="200" width="200" height="300" fill="url(#atmGrad)" rx="10"/>
  <rect x="320" y="220" width="160" height="100" fill="#1E3A8A" rx="5"/>
  <rect x="330" y="230" width="140" height="80" fill="#3B82F6" rx="3"/>
  
  <!-- ATM Screen -->
  <rect x="340" y="240" width="120" height="60" fill="#000" rx="2"/>
  <text x="400" y="275" text-anchor="middle" fill="#00FF00" font-family="monospace" font-size="12">WELCOME</text>
  
  <!-- Keypad -->
  <rect x="320" y="340" width="160" height="80" fill="#E5E7EB" rx="5"/>
  <circle cx="350" cy="360" r="8" fill="#6B7280"/>
  <circle cx="380" cy="360" r="8" fill="#6B7280"/>
  <circle cx="410" cy="360" r="8" fill="#6B7280"/>
  <circle cx="440" cy="360" r="8" fill="#6B7280"/>
  
  <!-- Card Slot -->
  <rect x="360" y="450" width="80" height="10" fill="#000" rx="2"/>
  
  <!-- Person 1 (using ATM) -->
  <circle cx="250" cy="350" r="25" fill="#D4A574"/>
  <rect x="235" y="375" width="30" height="50" fill="#3B82F6" rx="5"/>
  <rect x="225" y="425" width="15" height="40" fill="#1F2937"/>
  <rect x="260" y="425" width="15" height="40" fill="#1F2937"/>
  <rect x="220" y="390" width="20" height="30" fill="#D4A574"/>
  <rect x="260" y="390" width="20" height="30" fill="#D4A574"/>
  
  <!-- Person 2 (waiting) -->
  <circle cx="150" cy="380" r="25" fill="#F4A460"/>
  <rect x="135" y="405" width="30" height="50" fill="#059669" rx="5"/>
  <rect x="125" y="455" width="15" height="40" fill="#1F2937"/>
  <rect x="160" y="455" width="15" height="40" fill="#1F2937"/>
  <rect x="120" y="420" width="20" height="30" fill="#F4A460"/>
  <rect x="160" y="420" width="20" height="30" fill="#F4A460"/>
  
  <!-- Building in background -->
  <rect x="550" y="150" width="200" height="350" fill="#E5E7EB" rx="5"/>
  <rect x="570" y="180" width="30" height="40" fill="#3B82F6" rx="2"/>
  <rect x="620" y="180" width="30" height="40" fill="#3B82F6" rx="2"/>
  <rect x="670" y="180" width="30" height="40" fill="#3B82F6" rx="2"/>
  
  <!-- Bank Sign -->
  <rect x="320" y="150" width="160" height="40" fill="#1E40AF" rx="5"/>
  <text x="400" y="175" text-anchor="middle" fill="white" font-family="Arial" font-size="16" font-weight="bold">OAKLINE BANK</text>
</svg>
