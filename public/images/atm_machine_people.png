
<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <!-- ATM Machine Background -->
  <rect width="800" height="600" fill="#1e40af"/>
  <rect x="50" y="100" width="200" height="400" fill="#2d3748" rx="10"/>
  
  <!-- ATM Screen -->
  <rect x="75" y="150" width="150" height="100" fill="#000" rx="5"/>
  <rect x="85" y="160" width="130" height="80" fill="#1a365d" rx="3"/>
  <text x="150" y="190" text-anchor="middle" fill="#ffffff" font-family="Arial" font-size="12">OAKLINE BANK</text>
  <text x="150" y="210" text-anchor="middle" fill="#38a169" font-size="10">Welcome!</text>
  
  <!-- ATM Keypad -->
  <g fill="#4a5568">
    <rect x="85" y="280" width="25" height="20" rx="2"/>
    <rect x="120" y="280" width="25" height="20" rx="2"/>
    <rect x="155" y="280" width="25" height="20" rx="2"/>
    <rect x="190" y="280" width="25" height="20" rx="2"/>
  </g>
  
  <!-- People silhouettes -->
  <g fill="#4a5568" opacity="0.8">
    <!-- Person 1 (at ATM) -->
    <circle cx="180" cy="320" r="15"/>
    <rect x="170" y="335" width="20" height="40" rx="10"/>
    <rect x="165" y="340" width="10" height="25" rx="5"/>
    <rect x="185" y="340" width="10" height="25" rx="5"/>
    
    <!-- Person 2 (waiting) -->
    <circle cx="320" cy="350" r="12"/>
    <rect x="312" y="362" width="16" height="35" rx="8"/>
    <rect x="308" y="367" width="8" height="20" rx="4"/>
    <rect x="324" y="367" width="8" height="20" rx="4"/>
    
    <!-- Person 3 (walking) -->
    <circle cx="450" cy="380" r="10"/>
    <rect x="444" y="390" width="12" height="30" rx="6"/>
    <rect x="440" y="395" width="6" height="18" rx="3"/>
    <rect x="454" y="395" width="6" height="18" rx="3"/>
  </g>
  
  <!-- Bank building in background -->
  <rect x="500" y="200" width="250" height="300" fill="#2d3748" opacity="0.6"/>
  <rect x="520" y="220" width="30" height="40" fill="#4a5568" opacity="0.8"/>
  <rect x="560" y="220" width="30" height="40" fill="#4a5568" opacity="0.8"/>
  <rect x="600" y="220" width="30" height="40" fill="#4a5568" opacity="0.8"/>
  <rect x="640" y="220" width="30" height="40" fill="#4a5568" opacity="0.8"/>
  <rect x="680" y="220" width="30" height="40" fill="#4a5568" opacity="0.8"/>
  
  <text x="625" y="180" text-anchor="middle" fill="#ffffff" font-family="Arial" font-size="18" font-weight="bold">OAKLINE BANK</text>
</svg>
